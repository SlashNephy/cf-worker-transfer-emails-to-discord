{
  "name": "transfer-emails-to-discord",
  "version": "0.0.0",
  "private": true,
  "repository": "https://github.com/SlashNephy/cf-worker-transfer-emails-to-discord",
  "scripts": {
    "build": "tsc",
    "deploy": "wrangler deploy",
    "format": "concurrently -n format: -m 1 'yarn:format:*'",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "yarn lint:prettier --write",
    "lint": "concurrently -n lint: 'yarn:lint:*'",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "start": "wrangler dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20231002.0",
    "@slashnephy/eslint-config": "2.0.29",
    "@slashnephy/prettier-config": "1.0.18",
    "concurrently": "8.2.1",
    "eslint": "8.50.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "wrangler": "3.10.1"
  },
  "packageManager": "yarn@3.6.4",
  "eslintConfig": {
    "extends": "@slashnephy/eslint-config"
  }
}
